{
  "name": "browser-tech-shirtdesign",
  "version": "1.0.0",
  "description": "create an application that's is user friendly for all the browsers and devices",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prestart": "npm run dev",
    "poststart": "npm run scss",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run dev & npm run scss",
    "scss": "node-sass --include-path scss public/scss/styling.scss public/css/styling.css",
    "watch-scss": "nodemon -e scss -x \"npm run scss\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joanpadolina/browser-technologies-1920.git"
  },
